{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [	  
        {
          "background": "#0077c2",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b6",
          "properties": {
            "style": "full"
          },
          "style": "diamond",
          "template": " \uf07b  {{ .Path }}",
          "type": "path"
        },
		{
          "background": "#ebcc34",
		  "background_templates": [
            "{{ if contains \"prod\" .Context }}#ff0000{{ else }}#ebcc34{{ end }}"
          ],
          "foreground": "#000000",
		  "powerline_symbol": "\ue0b0",         
          "style": "powerline",
          "template": " \ufd31 {{.Context}} {{if .Namespace}}- {{.Namespace}}{{end}}",
          "trailing_diamond": "\ue0b0",
          "type": "kubectl"
        },
		{
		  "background": "#3e9022",
		  "background_templates": [
            "{{ if contains \"prod\" .Context }}#167c44{{ else }}#4ed289{{ end }}"
          ],
          "foreground": "#ffffff",
		  "powerline_symbol": "\ue0b0",         
          "style": "powerline",
          "template": " \uf5f7 {{.Context}} ",
		  "properties": {
            "always_enabled": true
          },
          "trailing_diamond": "\ue0b0",
          "type": "codefresh"
		},
		{
		  "background": "#3e9022",
		  "background_templates": [
            "{{ if contains \"dev\" .Context }}#e07df5{{ else }}#5c0f6c{{ end }}"
          ],
          "foreground": "#ffffff",
		  "powerline_symbol": "\ue0b0",         
          "style": "powerline",
          "template": " \ue710 {{ if contains \"dev\" .Context }}dev {{ else }}prod {{ end }} ",
		  "properties": {
            "always_enabled": true
          },
          "trailing_diamond": "\ue0b0",
          "type": "pulumi"
		},
		{
		  "type": "aws",
		  "style": "powerline",
		  "powerline_symbol": "\ue0b0",
		  "foreground": "#ffffff",
		  "background": "#3e9022",
		  "background_templates": [
            "{{ if contains \"prod\" .Profile }}#ff0000{{ else }}#3e9022{{ end }}"
          ],
		  "template": " \uf52c {{.Profile}}{{if .Region}}@{{.Region}}{{end}}"
		},
        {
          "background": "#2e9599",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FF9248{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#ff4500{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ],
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_max_length": 25,
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }}",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
	{
      "alignment": "right",
      "segments": [
        {
          "background": "#003543",
          "foreground": "#ffffff",
		  "properties": {
            "time_format": "_2 15:04:05"
          },
          "style": "plain",
          "template": " \uf64f {{ .CurrentDate | date .Format }} ",
          "type": "time"
        },
        {
          "background": "#83769c",
          "foreground": "#ffffff",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": " \ufa1e{{ .FormattedMs }} ",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#E06C75",
          "style": "plain",
          "template": "$ ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Folder }}",
  "final_space": true,
  "version": 2
}
